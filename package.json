{
  "name": "run-z",
  "version": "1.10.0",
  "description": "Run that, then this. package.json scripts and deps runner",
  "keywords": [
    "cli",
    "command",
    "npm-scripts",
    "package.json",
    "parallel",
    "run",
    "task"
  ],
  "homepage": "https://github.com/run-z/run-z",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:run-z/run-z.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/run-z/run-z/issues"
  },
  "type": "module",
  "main": "./dist/run-z.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./dist/run-z.js",
    "./builtins": "./dist/run-z.builtins.js"
  },
  "bin": {
    "run-z": "bin/run-z.js"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@proc7ts/logger": "^1.3.2",
    "@proc7ts/primitives": "^3.0.2",
    "@run-z/exec-z": "^1.4.1",
    "@run-z/log-z": "^2.1.0",
    "@run-z/optionz": "^2.4.0",
    "ansi-escapes": "^5.0.0",
    "chalk": "^5.0.1",
    "cli-spinners": "^2.6.1",
    "cli-truncate": "^3.1.0",
    "cross-spawn": "^7.0.3",
    "log-symbols": "^5.1.0",
    "npm-run-path": "^5.1.0",
    "path-key": "^4.0.0",
    "semver": "^7.3.7",
    "shell-quote": "^1.7.3",
    "string-width": "^5.1.2",
    "strip-ansi": "^7.0.1",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.1",
    "@run-z/eslint-config": "^3.0.1",
    "@run-z/prettier-config": "^2.0.0",
    "@run-z/rollup-helpers": "^2.0.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^16.11.41",
    "@types/semver": "^7.3.10",
    "@types/shell-quote": "^1.7.1",
    "@types/wrap-ansi": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.18.0",
    "eslint-plugin-jest": "^26.5.3",
    "gh-pages": "^4.0.0",
    "jest": "^28.1.1",
    "jest-junit": "^13.2.0",
    "jest-mock": "^28.1.1",
    "prettier": "^2.7.1",
    "rollup": "^2.75.6",
    "rollup-plugin-flat-dts": "^1.6.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.32.1",
    "run-z": "=1.10.0-bootstrap",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.5",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "typescript": "4.7"
      }
    }
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "bootstrap": "rollup -c",
    "build": "run-z +z bootstrap",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf \"index.d.ts?(.map)\" dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "format": "run-z +z --then prettier --write \"**/*.{json,md}\"",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z env:NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" --then jest",
    "test:all": "run-z test:command/--command2 test:script/--script2 --all3",
    "test:command": "run-z env=wrong env:NODE_OPTIONS=--no-warnings env:NODE_OPTIONS=--no-deprecation --then node ./src/spec/bin/ok.js --command1",
    "test:fail": "run-z --then node --no-warnings ./src/spec/bin/fail.js",
    "test:kill": "run-z --then node --no-warnings ./src/spec/bin/kill.js",
    "test:kill-with-code": "run-z --then node --no-warnings ./src/spec/bin/kill-with-code.js",
    "test:script": "node --no-warnings ./src/spec/bin/ok.js --script1",
    "test:silent": "run-z --then node --no-warnings ./src/spec/bin/silent.js --silent",
    "test:stale": "node --no-warnings ./src/spec/bin/stale.js",
    "z": "run-z +cmd:rollup,+cmd:typedoc,+cmd:eslint,+cmd:jest"
  }
}
